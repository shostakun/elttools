{
  "name": "elttools",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "check": "yarn cog-check && yarn prettier --check . && eslint --stats . && yarn type-check",
    "cog-check": "cog -I src/assets/cards --check src/assets/**/index.ts",
    "deploy": "rsync -av --delete dist/ $DEPLOY_DEST",
    "dev": "vite",
    "format": "yarn prettier --write .",
    "gen": "cog -I src/assets/cards -r src/assets/**/index.ts && yarn format",
    "lint": "eslint . --fix",
    "preview": "vite preview",
    "type-check": "vue-tsc --build --force"
  },
  "dependencies": {
    "@mdi/font": "7.4.47",
    "classnames": "^2.5.1",
    "core-js": "^3.37.1",
    "lodash": "^4.17.21",
    "roboto-fontface": "*",
    "vue": "^3.4.31",
    "vuetify": "^3.6.14"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@mdi/js": "^7.4.47",
    "@tsconfig/node22": "^22.0.0",
    "@types/lodash": "^4",
    "@types/node": "^22.9.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-vue": "^9.30.0",
    "npm-run-all2": "^7.0.1",
    "prettier": "3.5.0",
    "sass": "1.77.8",
    "sass-embedded": "^1.77.8",
    "typescript": "~5.6.3",
    "unplugin-fonts": "^1.1.1",
    "unplugin-vue-components": "^0.27.2",
    "unplugin-vue-router": "^0.10.0",
    "vite": "^5.4.10",
    "vite-plugin-vuetify": "^2.0.3",
    "vue-router": "^4.4.0",
    "vue-tsc": "^2.1.10"
  },
  "packageManager": "yarn@4.6.0"
}
